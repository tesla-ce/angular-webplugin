{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "dom-mediacapture-record"
    ],
    "paths": { "@angular/*": [ "./node_modules/@angular/*" ] },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
}
